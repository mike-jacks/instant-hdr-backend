                "parameters": [
                    {
                        "type": "string",
                        "description": "Project ID (UUID)",
                        "name": "project_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Processing options",
                        "name": "request",
                        "in": "body",
